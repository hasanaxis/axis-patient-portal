# Frontend Specific Environment Variables

# API Configuration
VITE_API_URL=http://localhost:5000/api/v1
VITE_WS_URL=ws://localhost:5000

# Application Settings
VITE_APP_NAME=Axis Imaging Patient Portal
VITE_APP_VERSION=1.0.0
VITE_COMPANY_NAME=Axis Imaging
VITE_CLINIC_ADDRESS=Mickleham, Victoria, Australia

# Feature Flags
VITE_ENABLE_SMS_AUTH=true
VITE_ENABLE_EMAIL_AUTH=false
VITE_ENABLE_BIOMETRIC_AUTH=false
VITE_ENABLE_APPOINTMENT_BOOKING=true
VITE_ENABLE_REPORT_DOWNLOAD=true
VITE_ENABLE_IMAGE_VIEWER=true
VITE_ENABLE_NOTIFICATIONS=true

# Google Maps (for clinic location)
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# Sentry (Error Tracking)
VITE_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id

# Analytics
VITE_GA_TRACKING_ID=UA-XXXXXXXXX-X

# Support
VITE_SUPPORT_EMAIL=support@axisimaging.com.au
VITE_SUPPORT_PHONE=+61390001234

# Development
VITE_MOCK_API=false
VITE_DEBUG=false